<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.layout.GridPane?>
<VBox alignment="CENTER" prefHeight="1080.0" prefWidth="1920.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.javafx.TitleController">
   <children>
      <GridPane fx:id="dataGrid" xmlns:fx="http://javafx.com/fxml" />
      <Text fill="#103ddd" stroke="#db2589" strokeType="OUTSIDE" strokeWidth="0.0" text="KEW&amp;J NATIONAL RAILROAD (KNRR)" textAlignment="CENTER">
         <font>
            <Font name="System Bold Italic" size="87.0" />
         </font>
         <VBox.margin>
            <Insets bottom="20.0" />
         </VBox.margin>
      </Text>
      <ImageView fitHeight="703.0" fitWidth="1720.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../images/titleScreen.png" />
         </image>
      </ImageView>
      <ButtonBar buttonMinWidth="50.0" layoutX="50.0" prefHeight="85.0" prefWidth="1885.0">
        <buttons>
            <Button fx:id="devModeButton" alignment="CENTER" contentDisplay="CENTER" defaultButton="true" onAction="#switchToSecondary" text="Dev Mode" textFill="#87888a" translateX="-1000.0">
               <font>
                  <Font size="36.0" />
               </font>
            </Button>
            <Button fx:id="primaryButton" alignment="CENTER" contentDisplay="CENTER" defaultButton="true" onAction="#switchToSecondary" text="Train Screen" textAlignment="CENTER" textFill="#103ddd">
               <font>
                  <Font size="36.0" />
               </font>
            </Button>
        </buttons>
      </ButtonBar>
      <Label fx:id="testLabel" prefHeight="116.0" prefWidth="302.0" text="Label" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
